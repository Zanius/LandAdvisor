<p id="notice"><%= notice %></p>

<h1>Short Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Parcel street</th>
      <th>Parcel street2</th>
      <th>Parcel city</th>
      <th>Parcel zip</th>
      <th>Parcel State</th>
      <th>Parcel county</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.parcel_street %></td>
        <td><%= lead.parcel_street2 %></td>
        <td><%= lead.parcel_city %></td>
        <td><%= lead.parcel_zip %></td>
        <td><%= lead.parcel_state %></td>
        <td><%= lead.parcel_county %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Short Form', new_lead_path %>
